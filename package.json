{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "starter kit for building web apps with react",
  "main": "src/index.js",
  "author": {
    "name": "Luis Angel Barrancos Ortiz (luisbar)",
    "email": "luisbar180492@gmail.com"
  },
  "contributors": [
    {
      "name": "Julian Gil Santos",
      "email": "gilsantosjulian@gmail.com"
    }
  ],
  "homepage": "https://gitlab.com/luisbar/react-starter-kit#README",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/luisbar/react-starter-kit.git"
  },
  "bugs": {
    "url": "https://gitlab.com/luisbar/react-starter-kit/issues"
  },
  "license": "ISC",
  "scripts": {
    "test": "jest",
    "commit": "git cz",
    "start": "webpack-dev-server --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "prettier": "prettier 'src/**/*.{ts,tsx}' --write",
    "lint": "eslint --debug 'src/**/*.{ts,tsx}'",
    "lint:write": "eslint --debug 'src/**/*.{ts,tsx}' --fix",
    "watcher": "node scripts/watcher/",
    "init": "npm-run-all --parallel start watcher"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run prettier",
      "npm run lint:write",
      "git add"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "clean-webpack-plugin": "^2.0.1",
    "commitizen": "^3.0.7",
    "css-loader": "^1.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^4.3.0"
  },
  "dependencies": {
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "navi": "^0.12.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-navi": "^0.12.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
